XML_CATALOG_FILES=/opt/guengel/etc/xml/catalog.xml
CC=cc
CXX=CC
CPPFLAGS='-I /usr/xpg4/include -I /usr/sfw/include'
CXXFLAGS='-xO0 -g +w'
LDFLAGS='-L /usr/xpg4/lib -R /usr/xpg4/lib -L/usr/sfw/lib -R/usr/sfw/lib'
PATH=/usr/bin:/usr/sbin:/usr/ccs/bin
export XML_CATALOG_FILES CC CXX CXXFLAGS PATH CPPFLAGS LDFLAGS
