# -*- makefile -*-
#
# $Id$

all: README ChangeLog aclocal.m4 configure config.h.in Makefile.in

README:
	touch README

ChangeLog:
	touch ChangeLog

aclocal.m4: configure.ac
	aclocal

configure: configure.ac
	autoconf

config.h.in: configure.ac
	autoheader

Makefile.in: yapet/Makefile.am ui/Makefile.am crypt/Makefile.am tests/Makefile.am doc/Makefile.am Makefile.am
	automake -a -c -f --gnu
